{
    "manifest_version": 3,
    "name": "Marketplace Negotiator Bot",
    "version": "1.0",
    "description": "Scrapes marketplace items and generates negotiation messages.",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}